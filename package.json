{
  "name": "logistic-bdt-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-dev": "ng serve --proxy-config proxy.config.json --port 42331 -o",
    "start": "node server.js",
    "watch": "ng build --watch --configuration development",
    "build": "ng build",
    "generate": "npm run generate:defs && npm run generate:meta",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package sample-polkadotjs-typegen/interfaces --input ./src/edgeware/interfaces --endpoint ./edgeware.json",
    "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package sample-polkadotjs-typegen/interfaces --endpoint ./edgeware.json --output ./src/edgeware/interfaces",
    "lint": "tsc --noEmit --pretty",
    "heroku-postbuild": "ng build --configuration=production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.3.6",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.3.6",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@noble/hashes": "^1.0.0",
    "@polkadot/api": "^8.6.2",
    "@polkadot/extension-dapp": "^0.43.2",
    "@polkadot/keyring": "^9.3.1",
    "@polkadot/networks": "^9.3.1",
    "@polkadot/types": "^8.6.2",
    "@polkadot/ui-keyring": "^2.3.1",
    "@polkadot/ui-settings": "^2.3.1",
    "@polkadot/util": "^9.3.1",
    "@polkadot/util-crypto": "^9.3.1",
    "@types/d3-shape": "^3.0.2",
    "@zxing/browser": "^0.1.1",
    "@zxing/library": "^0.19.1",
    "@zxing/ngx-scanner": "^3.5.0",
    "autoprefixer": "10.4.5",
    "bootstrap": "^5.1.3",
    "crypto-js": "^4.1.1",
    "d3": "^7.4.4",
    "eva-icons": "^1.1.3",
    "express": "^4.18.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.0",
    "jszip": "^3.9.1",
    "ng-bootstrap": "^1.6.3",
    "ng2-ckeditor": "^1.3.6",
    "ngx-bootstrap": "^8.0.0",
    "ngx-toastr": "^14.3.0",
    "path": "^0.12.7",
    "remixicon": "^2.5.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "tslint": "^6.1.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@polkadot/dev": "^0.66.32",
    "@polkadot/typegen": "^8.6.2",
    "@types/crypto-js": "^4.1.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
